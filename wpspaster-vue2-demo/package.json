    "dayjs": "^1.11.6",
    "driver.js": "0.9.5",
    "dropzone": "5.5.1",
    "echarts": "^5.4.0",
    "element-ui": "^2.15.12",
    "file-saver": "2.0.1",
    "fuse.js": "3.4.4",
    "js-cookie": "2.2.0",
    "jsonlint": "1.6.3",
    "jszip": "3.2.1",
    "normalize.css": "7.0.0",
    "nprogress": "0.2.0",
    "path-to-regexp": "2.4.0",
    "screenfull": "4.2.0",
    "script-loader": "0.7.2",
    "socket.io-client": "4.5.4",
    "sortablejs": "1.8.4",
    "vue": "2.6.10",
    "vue-count-to": "1.0.13",
    "vue-router": "3.0.2",
    "vue-socket.io": "3.0.7",
    "vue-splitpane": "1.0.4",
